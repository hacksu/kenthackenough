{
  "name": "kenthackenough",
  "version": "1.0.0",
  "description": "The world's best hackathon website.",
  "scripts": {
    "test": "vagrant ssh -c 'cd /var/www/kenthackenough && env NODE_ENV='test' mocha test/test.js'",
    "server": "vagrant ssh -c 'cd /var/www/kenthackenough && env NODE_ENV='development' node app.js | bunyan'",
    "start": "vagrant ssh -c 'cd /var/www/kenthackenough && pm2 startOrRestart processes.json'",
    "stop": "vagrant ssh -c 'cd /var/www/kenthackenough && pm2 delete api'",
    "build": "vagrant ssh -c 'cd /var/www/kenthackenough && npm rebuild'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hacksu/kenthackenough.git"
  },
  "author": "Paul Dilyard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hacksu/kenthackenough/issues"
  },
  "homepage": "http://khe.io",
  "devDependencies": {
    "mocha": "2.3.x",
    "should": "7.1.x",
    "supertest": "1.1.x"
  },
  "dependencies": {
    "bcrypt": "0.8.x",
    "body-parser": "1.14.x",
    "bunyan": "1.5.x",
    "compression": "1.5.x",
    "cors": "2.7.x",
    "csv": "0.4.x",
    "express": "4.13.x",
    "express-zip": "2.0.x",
    "extend": "3.0.x",
    "flow": "0.2.x",
    "marked": "0.3.x",
    "mongoose": "4.1.x",
    "multiparty": "4.1.x",
    "node-gcm": "0.12.x",
    "redis": "2.0.x",
    "sendgrid": "1.9.x",
    "socket.io": "1.3.x",
    "to-csv": "0.1.x",
    "uuid": "2.0.x",
    "validate": "3.0.x"
  }
}
