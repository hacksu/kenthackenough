{
  "name": "kenthackenough",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src",
    "build": "tsc && copyfiles -s -u 1 src/**/* dist -e '**/*.ts' -e '**/*.js'",
    "start": "node -r ./dist.paths.js dist",
    
    "docker:up": "docker-compose -f \"docker-compose.yml\" up -d --build",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "production:up": "docker-compose -f \"docker-production.yml\" up -d --build",
    "production:restart": "docker-compose -f \"docker-production.yml\" restart",
    "production:down": "docker-compose -f \"docker-production.yml\" down",

    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.6",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.39.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "copyfiles": "^2.4.1",
    "js-yaml": "^4.1.0",
    "json5": "^1.0.1",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}
