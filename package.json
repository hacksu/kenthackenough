{
  "name": "kenthackenough",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "gql": "npm run node src/graphql/schema",
    "node": "ts-node -r tsconfig-paths/register",
    "dev": "ts-node-dev -r tsconfig-paths/register src",
    "start": "npm run node src",
    "build": "npm run gql && rimraf ./dist && tsc; copyfiles -s -e \"./src/**/*.ts\" -e \"./src/**/*.js\" -u 1 \"./src/**/*\" ./dist",
    "prod": "node -r ./tsconfig.paths.js dist",
    "compose-dev": "docker-compose -f ./docker/compose/dev.yml -p khe --env ./docker/docker.env",
    "compose-prod": "docker-compose -f ./docker/compose/prod.yml -p khe --env ./docker/docker.env",
    "docker:up": "npm run compose-dev up -d --build",
    "docker:down": "npm run compose-dev down",
    "production:up": "npm run compose-prod up -d --build",
    "production:down": "npm run compose-prod down",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.4.6",
    "apollo-server-express": "^3.3.0",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "graphql": "^15.5.3",
    "lodash": "^4.17.21",
    "marked": "^3.0.4",
    "mongoose": "^6.0.6",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.39.2"
  },
  "devDependencies": {
    "@graphql-codegen/core": "^2.1.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-tools/load-files": "^6.4.0",
    "@graphql-tools/merge": "^8.1.2",
    "@graphql-tools/schema": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.2",
    "@types/yargs": "^17.0.2",
    "copyfiles": "^2.4.1",
    "js-yaml": "^4.1.0",
    "json5": "^1.0.1",
    "rimraf": "^2.7.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3",
    "yargs": "^16.2.0"
  }
}
