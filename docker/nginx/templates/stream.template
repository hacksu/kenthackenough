
server {
    listen 6379 so_keepalive=on;
    proxy_connect_timeout 2s;
    proxy_pass redis;
    proxy_timeout 10m;
}

server {
    listen 27017 so_keepalive=on;
    listen 27020 so_keepalive=on;
    proxy_connect_timeout 2s;
    proxy_pass mongo;
    proxy_timeout 10m;
}


upstream redis {
    server redis:6379;
}

upstream mongo {
    server mongo:27017;
}
